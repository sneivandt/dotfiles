{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command":
            {
                "action": "commandPalette"
            },
            "keys": "ctrl+shift+p"
        },
        {
            "command":
            {
                "action": "newTab",
                "index": 0
            },
            "keys": "ctrl+shift+t"
        },
        {
            "command": "closePane",
            "keys": "ctrl+shift+w"
        },
        {
            "command":
            {
                "action": "nextTab"
            },
            "keys": "ctrl+shift+n"
        },
        {
            "command": "duplicateTab",
            "keys": "ctrl+shift+d"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{505ae8ff-4b0a-4666-8d59-1c476f7ffe0d}",
